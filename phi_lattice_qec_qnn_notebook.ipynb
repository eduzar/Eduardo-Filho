    {
      "cell_type": "markdown",
      "source": ["## 1. Generate φ-Quasicrystal Graph (N=50,000)"],
      "metadata": {}
    },
    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "from sklearn.neighbors import NearestNeighbors\n",
        "import networkx as nx\n",
        "\n",
        "N = 50000\n",
        "phi = (1 + np.sqrt(5)) / 2\n",
        "indices = np.arange(N)\n",
        "x = (indices / phi) % 100\n",
        "y = (indices * phi) % 1.0\n",
        "points = np.column_stack([x, y])\n",
        "\n",
        "# k-NN Graph (k=6)\n",
        "nbrs = NearestNeighbors(n_neighbors=6, algorithm='ball_tree').fit(points)\n",
        "adj = nbrs.kneighbors_graph(points, mode='connectivity')\n",
        "G = nx.from_scipy_sparse_matrix(adj)\n",
        "\n",
        "print(f\"φ-Quasicrystal Graph: {N} nodes, {G.number_of_edges()} edges\")\n",
        "\n",
        "# Plot sample (first 1000 nodes)\n",
        "plt.figure(figsize=(10,6))\n",
        "sample = slice(0, 1000)\n",
        "plt.scatter(points[sample, 0], points[sample, 1], c='gold', s=1, alpha=0.6)\n",
        "plt.title(\"φ-Beatty Quasicrystal Graph (N=50,000 — Sample)\", color='white')\n",
        "plt.gca().set_facecolor('black')\n",
        "plt.axis('off')\n",
        "plt.show()"
      ],
      "metadata": {},
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "markdown",
      "source": ["## 2. Quantum Walk + Mixed Damping"],
      "metadata": {}
    },
    {
      "cell_type": "code",
      "source": [
        "from scipy.sparse import lil_matrix, kron, eye\n",
        "\n",
        "n = N\n",
        "H = np.array([[1,1],[1,-1]]) / np.sqrt(2)\n",
        "C = kron(eye(n), H)\n",
        "\n",
        "S = lil_matrix((2*n, 2*n))\n",
        "for i, j in G.edges():\n",
        "    S[2*j + 1, 2*i] = 1\n",
        "    S[2*j,     2*i + 1] = 1\n",
        "S = S.tocsr()\n",
        "U = S @ C\n",
        "\n",
        "psi = np.zeros(2*n, dtype=complex)\n",
        "psi[2*(N//2)] = 1.0\n",
        "\n",
        "gamma_phase = 0.001\n",
",
        "gamma_amp = 0.0005\n",
        "steps = 100\n",
        "coherence_history = []\n",
        "\n",
        "for t in range(steps):\n",
        "    psi = U @ psi\n",
        "    \n",
        "    # Phase damping\n",
        "    for i in range(n):\n",
        "        idx0, idx1 = 2*i, 2*i+1\n",
        "        psi[idx0] *= np.exp(1j * gamma_phase * np.random.randn())\n",
        "        psi[idx1] *= np.exp(1j * gamma_phase * np.random.randn())\n",
        "    \n",
        "    # Amplitude damping\n",
        "    decay = np.exp(-gamma_amp * t)\n",
        "    psi *= np.sqrt(decay)\n",
        "    psi /= np.linalg.norm(psi) + 1e-12\n",
        "    \n",
        "    rho = np.outer(psi, psi.conj())\n",
        "    off_diag = rho - np.diag(np.diag(rho))\n",
        "    coherence = np.sum(np.abs(off_diag))\n",
        "    coherence_history.append(coherence)\n",
        "\n",
        "print(f\"Final coherence: {coherence_history[-1]:.6f}\")"
      ],
      "metadata": {},
      "execution_count": null,
      "outputs": []
    },
