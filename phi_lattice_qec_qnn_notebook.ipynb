    {
      "cell_type": "markdown",
      "source": ["## 5. QNN with φ-Coherence Attention"],
      "metadata": {}
    },
    {
      "cell_type": "code",
      "source": [
        "import torch\n",
        "import torch.nn as nn\n",
        "\n",
        "class PhiQNN(nn.Module):\n",
        "    def __init__(self, coherence_final):\n",
        "        super().__init__()\n",
        "        self.phi_attn = nn.Parameter(torch.tensor(coherence_final))\n",
        "        self.fc = nn.Linear(N, 1)\n",
        "    \n",
        "    def forward(self, x):\n",
        "        attn = torch.tanh(self.phi_attn)\n",
        "        return self.fc(x * attn)\n",
        "\n",
        "model = PhiQNN(coherence_history[-1])\n",
        "X = torch.tensor(points, dtype=torch.float32)\n",
        "y = torch.sin(X[:,0] * np.pi / 50).unsqueeze(1)\n",
        "\n",
        "optimizer = torch.optim.Adam(model.parameters(), lr=0.005)\n",
        "criterion = nn.MSELoss()\n",